ents/EditIncome.vue","name":null,"original":{"line":112,"column":0},"generated":{"line":119,"column":0}},{"source":"components/EditIncome.vue","name":null,"original":{"line":113,"column":0},"generated":{"line":120,"column":0}},{"source":"components/EditIncome.vue","name":null,"original":{"line":101,"column":0},"generated":{"line":120,"column":9}},{"source":"components/EditIncome.vue","name":null,"original":{"line":114,"column":0},"generated":{"line":121,"column":0}},{"source":"components/EditIncome.vue","name":null,"original":{"line":115,"column":0},"generated":{"line":122,"column":0}},{"source":"components/EditIncome.vue","name":null,"original":{"line":75,"column":0},"generated":{"line":123,"column":0}},{"source":"components/EditIncome.vue","name":null,"original":{"line":65,"column":0},"generated":{"line":124,"column":0}}],"sources":{"components/EditIncome.vue":"<template>\n\n    <div>\n        <div class=\"modal fade\" id=\"EditModal\" tabindex=\"-1\" aria-labelledby=\"ModalLabel\" aria-hidden=\"true\">\n            <div class=\"modal-dialog\">\n                <div class=\"modal-content\">\n                    <div class=\"modal-header\">\n                        <h5 class=\"modal-title\">Modifier un revenu</h5>\n                        <button type=\"button\" class=\"btn-close\" data-bs-dismiss=\"modal\" aria-label=\"Close\"></button>\n                    </div>\n                    <div class=\"modal-body\">\n                        <form v-on:submit.prevent=\"checkForm\">\n                            <div class=\"input-group mb-4\">\n                                <label for=\"newIncome\" class=\"form-label pe-3\">Montant :</label>\n                                <input type=\"text\" class=\"form-control\" id=\"newIncome\" v-model=\"amount\">\n                                <span class=\"input-group-text\" id=\"basic-addon2\">€</span>\n                            </div>\n                            <div class=\"mb-4\">\n                                <select class=\"form-select\" aria-label=\"month\" v-model=\"month\">\n                                    <option selected>Mois</option>\n                                    <option value=\"Janvier\">Janvier</option>\n                                    <option value=\"Février\">Février</option>\n                                    <option value=\"Mars\">Mars</option>\n                                    <option value=\"Avril\">Avril</option>\n                                    <option value=\"Mai\">Mai</option>\n                                    <option value=\"Juin\">Juin</option>\n                                    <option value=\"Juillet\">Juillet</option>\n                                    <option value=\"Août\">Août</option>\n                                    <option value=\"Septembre\">Septembre</option>\n                                    <option value=\"Octobre\">Octobre</option>\n                                    <option value=\"Novembre\">Novembre</option>\n                                    <option value=\"Décembre\">Décembre</option>\n                                </select>\n                            </div>\n                            <div class=\"mb-4\">\n                                <select class=\"form-select\" aria-label=\"year\" v-model=\"year\">\n                                    <option selected>Année</option>\n                                    <option value=\"2020\">2020</option>\n                                    <option value=\"2021\">2021</option>\n                                    <option value=\"2022\">2022</option>\n                                </select>\n                            </div>\n                            <div class=\"alert-error\">\n                                <ul class=\"error-list\" v-if=\"errorList.length > 0\">\n                                    <li v-for=\"error in errorList\" v-bind:key=\"error.id\">{{ error.message }}</li>\n                                </ul>\n                            </div>\n                        </form>\n                    </div>\n                    <div class=\"modal-footer\">\n                        <button type=\"button\" class=\"btn btn-secondary\" data-bs-dismiss=\"modal\">Annuler</button>\n                        <button type=\"button\" class=\"btn btn-primary\" v-on:click=\"update\">Modifier</button>\n                    </div>\n                </div>\n            </div>\n        </div>\n    </div>\n    \n</template>\n\n<script>\n\nimport ApiClient from '../services/ApiClient';\n\nexport default {\n    props: [\"incomeID\"],\n    data: function () {\n        return {\n            month: \"\",\n            year: \"\",\n            amount: \"\",\n            errorList: []\n        };\n    },\n    methods: {\n      update() {\n\n            this.errorList = [];\n\n            if (this.amount.length < 1) {\n                this.errorList.push({\n                    id: this.errorList.length + 1, \n                    message: \"Veuillez renseigner un montant\"\n                });\n            }\n\n            if (this.month.length < 1) {\n                this.errorList.push({\n                    id: this.errorList.length + 1, \n                    message: \"Vous n'avez pas indiqué le mois\"\n                });\n            }\n            if (this.year.length < 1) {\n                this.errorList.push({\n                    id: this.errorList.length + 1, \n                    message: \"Vous n'avez pas indiqué l'année\"\n                });\n            }\n\n            if (this.errorList.length === 0) {\n                ApiClient.post('/income/update/'+this.incomeID, {\n                    month: this.month,\n                    year: this.year,\n                    amount: this.amount,\n                })\n                .then((response) => {\n                    console.log(response);\n                    if (response.request.status == 200) {\n                        location.reload();\n                    } else {\n                        console.log('erreur coté api');\n                    }\n                })\n            }\n        }\n    }\n    \n}\n</script>\n\n<style lang=\"scss\" scoped>\n    .error-list {\n        color: red;\n        list-style: none;\n        text-align: left;\n    }\n</style>"},"lineCount":null}},"error":null,"hash":"9f1e1abca605d8a552dd1f1788534f81","cacheData":{"env":{}}}